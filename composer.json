{
  "name": "yieldstudio/laravel-expo-notifier",
  "description": "Easily send Expo notifications with Laravel.",
  "type": "plugin",
  "keywords": [
    "yieldstudio",
    "laravel",
    "notifier",
    "notification",
    "expo"
  ],
  "homepage": "https://github.com/YieldStudio/laravel-expo-notifier",
  "license": "MIT",
  "authors": [
    {
      "name": "James Hemery",
      "email": "james@yieldstudio.fr",
      "homepage": "https://yieldstudio.fr",
      "role": "Developer"
    },
    {
      "name": "David Tang",
      "email": "david@yieldstudio.fr",
      "homepage": "https://yieldstudio.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "nesbot/carbon": ">=2.62.1"
  },
  "require-dev": {
    "ciareis/bypass": "^1.0",
    "dg/bypass-finals": "^1.4",
    "guzzlehttp/guzzle": "^7.8",
    "laravel/pint": "^1.3",
    "orchestra/testbench": "^7.0|^8.0|^9.0.2",
    "pestphp/pest": "^1.21|^2.0",
    "phpunit/phpunit": "^9.4 || ^10.5 || ^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "YieldStudio\\LaravelExpoNotifier\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YieldStudio\\LaravelExpoNotifier\\Tests\\": "tests"
    }
  },
  "scripts": {
    "format": "pint",
    "test": "vendor/bin/pest",
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "YieldStudio\\LaravelExpoNotifier\\ExpoNotificationsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
